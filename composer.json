{
  "name": "greg-md/php-framework",
  "type": "library",
  "description": "Greg Framework provides a lightweight engine for fast creating powerful apps.",
  "keywords": [ "greg-md", "greg-php", "php-framework", "greg-framework", "framework", "php", "web-artisans" ],
  "homepage": "https://github.com/greg-md/php-framework",
  "license": "MIT",
  "authors": [
    { "name": "Grigorii Duca", "email": "contact@greg.md", "homepage": "http://greg.md/" }
  ],
  "require": {
    "php": "^7.3",
    "greg-md/php-router": "dev-master",
    "greg-md/php-support": "dev-master",
    "greg-md/php-dependency-injection": "dev-master",
    "symfony/console": "^4"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "symfony/var-dumper": "^4"
  },
  "autoload": {
    "psr-4": {
      "Greg\\Framework\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Greg\\Framework\\": "tests/"
    }
  }
}
